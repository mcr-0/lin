generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
}

model conversions {
  id             Int      @id @default(autoincrement())
  createdat      DateTime @default(now()) @db.Timestamptz(0)
  offer_id       String?
  offer_name     String?
  affiliate_id   String?
  source         String?
  session_ip     String?
  payout         Float?
  aff_sub        String?
  aff_sub2       String?
  aff_sub3       String?
  aff_sub4       String?
  aff_sub5       String?
}

